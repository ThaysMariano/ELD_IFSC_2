# Loading std.standard
# Loading std.textio(body)
# Loading ieee.std_logic_1164(body)
# Loading work.maquinaestado(arch_maqest)
add wave -position end  sim:/maquinaestado/clk
add wave -position end  sim:/maquinaestado/rst
add wave -position end  sim:/maquinaestado/x
add wave -position end  sim:/maquinaestado/y
add wave -position end  sim:/maquinaestado/pr_state
add wave -position end  sim:/maquinaestado/nx_state
force -freeze sim:/maquinaestado/clk 1 0, 0 {50 ps} -r 100
run
force -freeze sim:/maquinaestado/x 1 0
run
force -freeze sim:/maquinaestado/x 2 0
run
run
force -freeze sim:/maquinaestado/x 0 0
run
force -freeze sim:/maquinaestado/x 2 0
run
force -freeze sim:/maquinaestado/x 1 0
run
force -freeze sim:/maquinaestado/x 2 0
run
force -freeze sim:/maquinaestado/x 1 0
run
run


